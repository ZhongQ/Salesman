<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ye_ji_trend"
    style="@style/Layout_Base_Style"
    tools:context="com.salesman.activity.work.YeJiTrendActivity">

    <include
        android:id="@+id/top_view_trend"
        layout="@layout/common_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView style="@style/SplitStrip_20" />

            <include layout="@layout/view_trend_part1" />

            <ImageView style="@style/SplitStrip_20" />

            <include layout="@layout/view_trend_part2" />

            <ImageView style="@style/SplitStrip_20" />

            <include layout="@layout/view_trend_part3" />

            <ImageView style="@style/SplitStrip_20" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>
